{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "hgcommit",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "title"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "section"
          }
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "HG: .*"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\n]+"
    },
    "section_key": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "Summary"
            },
            {
              "type": "STRING",
              "value": "Test Plan"
            },
            {
              "type": "STRING",
              "value": "Reviewers"
            },
            {
              "type": "STRING",
              "value": "Subscribers"
            },
            {
              "type": "STRING",
              "value": "Tasks"
            },
            {
              "type": "STRING",
              "value": "Tags"
            },
            {
              "type": "STRING",
              "value": " Differential Revision"
            },
            {
              "type": "STRING",
              "value": "Privacy Context Container"
            },
            {
              "type": "STRING",
              "value": "Reviewed By"
            },
            {
              "type": "STRING",
              "value": "Dependencies"
            },
            {
              "type": "STRING",
              "value": "GSD Projects"
            }
          ]
        }
      }
    },
    "section_value": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "text"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "title": {
      "type": "SYMBOL",
      "name": "section_value"
    },
    "section": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "key",
          "content": {
            "type": "SYMBOL",
            "name": "section_key"
          }
        },
        {
          "type": "FIELD",
          "name": "separator",
          "content": {
            "type": "STRING",
            "value": ":"
          }
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "section_value"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}